{
  "functions": {
    "api/dos.py": {
      "maxDuration": 60,
      "memory": 1024
    }
  },
  "rewrites": [
    {
      "source": "/dos",
      "destination": "/public/dos-panel.html"
    },
    {
      "source": "/api/dos/(.*)",
      "destination": "/api/dos.py"
    },
    {
      "source": "/(.*)",
      "destination": "/api/dos.py"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "Access-Control-Allow-Origin", "value": "*" },
        { "key": "Access-Control-Allow-Methods", "value": "GET,POST,OPTIONS" }
      ]
    }
  ]
}